{
  "name": "ClinVar Curator v2",
  "action": {},
  "manifest_version": 3,
  "version": "2.0",
  "description": "Capture SCV/VCV notes from ClinVar variant screen in a google sheet.",
  "background": {
    "service_worker": "background.js"
  },
  "oauth2": {
    "client_id": "974091131481-ffhgjpl8vh91e5qber2486eikrhf42ec.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/spreadsheets"]
  },
  "permissions": [
    "identity",
    "identity.email",
    "storage",
    "declarativeContent"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.ncbi.nlm.nih.gov/clinvar/variation/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "key" : ""
}